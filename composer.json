    "source": "https://github.com/TildBJ/sz_ebook"
  },
  "require": {
    "typo3/cms": ">=6.2.0,<7.99.99"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^2.7",
    "phpunit/phpunit": "^5.7"
  },
  "autoload": {
    "psr-4": {
      "TildBJ\\SzEbook\\": "Classes"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TildBJ\\SzEbook\\Tests\\": "Tests"
    }
